{
  "reviewStoragePath": ".reviews",
  "logging": {
    "level": "INFO",
    "toFile": false,
    "toConsole": false,
    "filePath": "./logs/custom-reviewer.log"
  },
  "persistReviewPrompts": false,
  "reviewer": {
    "type": "claude",
    "cliPath": "claude",
    "model": "opus",
    "timeout": 600000,
    "enableResume": true
  }
}